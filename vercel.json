{
  "functions": {
    "app/api/processQueue/route.ts": {
      "maxDuration": 30
    },
    "app/api/worker/route.ts": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/worker",
      "schedule": "*/1 * * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}